{
  "name": "dlite-web",
  "version": "0.1.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "postinstall": "webpack -p",
    "start": "node index.js",
    "webpack-watch": "webpack -w",
    "dev": "concurrently --kill-others \"npm run webpack-watch\" \"nodemon index.js\"",
    "test": "npm run test:client && npm run test:server && npm run test:acceptance",
    "test:server": "node_modules/.bin/mocha --recursive test/server",
    "test:client": "node_modules/.bin/mocha --compilers js:babel-core/register --recursive test/client",
    "test:acceptance": "node_modules/.bin/cucaroo",
    "test:coverage": "npm run test:client && npm run test:unit:coverage && npm run test:bdd:coverage && node test/support/coverage.js",
    "test:unit:coverage": "node_modules/.bin/istanbul cover -dir=coverage/unit node_modules/.bin/_mocha -- --recursive test/server",
    "test:bdd:coverage": "node_modules/.bin/istanbul cover -dir=coverage/bdd node_modules/.bin/cucaroo",
    "test:security": "node_modules/.bin/nsp check",
    "lint": "node_modules/.bin/jshint test/ server/ server.js index.js",
    "schema:export": "node bin/export-db-schema.js"
  },
  "author": "Kane Baccigalupi",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-generator": "^6.26.0",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.24.1",
    "compass-mixins": "^0.12.10",
    "concurrently": "^3.4.0",
    "css-loader": "^0.28.4",
    "cssurf": "https://github.com/CDTDigital/cssurf.git",
    "cucaroo": "^0.4.1",
    "enzyme": "^2.9.1",
    "extract-text-webpack-plugin": "^2.1.0",
    "istanbul": "^0.4.5",
    "jsdom": "11.2.0",
    "jsdom-global": "3.0.2",
    "jshint": "^2.9.4",
    "knex": "^0.13.0",
    "mocha": "^3.4.2",
    "node-horseman": "^3.3.0",
    "node-mocks-http": "^1.6.4",
    "node-sass": "^4.5.3",
    "nodemon": "^1.11.0",
    "nsp": "^2.6.3",
    "pg": "^7.3.0",
    "pg-json-schema-export": "^0.13.7",
    "phantomjs": "^2.1.7",
    "react-test-renderer": "^15.6.1",
    "sass-loader": "^6.0.5",
    "sinon": "^3.2.1",
    "style-loader": "^0.18.1",
    "webpack": "^2.6.0"
  },
  "dependencies": {
    "body-parser": "^1.17.2",
    "dotenv": "^4.0.0",
    "express": "^4.15.3",
    "helmet": "^3.6.1",
    "jsonwebtoken": "^7.4.1",
    "morgan": "^1.8.2",
    "passport": "^0.3.2",
    "passport-jwt": "^2.2.1",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-redux": "^5.0.5",
    "react-router-dom": "^4.1.2",
    "redux": "^3.7.2"
  }
}
